numero = int(input('Digite um numero: '))
cont = 0
for numero2 in range(1, numero + 1):
   
   if numero % numero2 == 0:
       cont += 1
       print('\033[34m{}\033[m'.format(numero2), end=' ')
   else:
       print('\033[31m{}\033[m'.format(numero2), end= ' ')
   if cont == 2:
       c = 'É primo'
   elif cont > 2:
       c = 'Não é primo'
print('\033[1;32m{}\033[m'.format(c))
